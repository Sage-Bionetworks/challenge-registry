{
  "projectType": "application",
  "root": "apps/web-app",
  "sourceRoot": "apps/web-app/src",
  "prefix": "challenge-registry",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/web-app",
        "index": "apps/web-app/src/index.html",
        "main": "apps/web-app/src/main.ts",
        "polyfills": "apps/web-app/src/polyfills.ts",
        "tsConfig": "apps/web-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/web-app/src/assets",
          {
            "input": "libs/shared/assets/src/assets",
            "glob": "**/*",
            "output": "assets"
          },
          {
            "input": "libs/shared/assets/src",
            "glob": "favicon.ico",
            "output": ""
          }
        ],
        "styles": [
          "apps/web-app/src/styles.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/web-app/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/web-app/src/environments/environment.ts",
              "with": "apps/web-app/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/web-app/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "web-app:build:production"
        },
        "development": {
          "browserTarget": "web-app:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "publicHost": "http://localhost:4200"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "web-app:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/web-app/src/**/*.ts",
          "apps/web-app/src/**/*.html"
        ]
      }
    },
    "lint-fix": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/web-app/src/**/*.ts",
          "apps/web-app/src/**/*.html"
        ],
        "fix": true
      }
    },
    "serve-mfe": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx serve web-app"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "shared-assets"
  ]
}
