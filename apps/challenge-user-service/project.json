{
  "root": "apps/challenge-user-service",
  "sourceRoot": "apps/challenge-user-service/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nxrocks/nx-spring-boot:build",
      "options": {
        "root": "apps/challenge-user-service",
        "args": ["-e"]
      }
    },
    "test": {
      "executor": "@nxrocks/nx-spring-boot:test",
      "options": {
        "root": "apps/challenge-user-service"
      }
    },
    "clean": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "options": {
        "root": "apps/challenge-user-service"
      }
    },
    "format": {
      "executor": "@nxrocks/nx-spring-boot:format",
      "options": {
        "root": "apps/challenge-user-service"
      }
    },
    "format-check": {
      "executor": "@nxrocks/nx-spring-boot:format-check",
      "options": {
        "root": "apps/challenge-user-service"
      }
    },
    "run": {
      "executor": "@nxrocks/nx-spring-boot:run",
      "options": {
        "root": "apps/challenge-user-service"
      }
    },
    "serve": {
      "executor": "@nxrocks/nx-spring-boot:serve",
      "options": {
        "root": "apps/challenge-user-service",
        "args": ["-X"]
      }
    },
    "build-image": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "context": "apps/challenge-user-service",
        "push": false,
        "tags": [
          "sagebionetworks/challenge-user-service:latest"
        ]
      }
    },
    "build-info": {
      "executor": "@nxrocks/nx-spring-boot:buildInfo",
      "options": {
        "root": "apps/challenge-user-service"
      }
    }
  },
  "tags": []
}
