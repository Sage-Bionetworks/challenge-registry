{
  "root": "apps/db-client",
  "sourceRoot": "apps/db-client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/db-client",
        "main": "apps/db-client/src/index.ts",
        "tsConfig": "apps/db-client/tsconfig.app.json",
        "assets": ["apps/db-client/*.md"]
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "db-client:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/db-client/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/db-client"],
      "options": {
        "jestConfig": "apps/db-client/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
