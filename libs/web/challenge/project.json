{
  "projectType": "library",
  "root": "libs/web/challenge",
  "sourceRoot": "libs/web/challenge/src",
  "prefix": "challenge-registry",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/challenge"],
      "options": {
        "jestConfig": "libs/web/challenge/jest.config.js",
        "passWithNoTests": true,
        "runInBand": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/challenge/src/**/*.ts",
          "libs/web/challenge/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
