{
  "projectType": "library",
  "root": "libs/web/about",
  "sourceRoot": "libs/web/about/src",
  "prefix": "challenge-registry",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/about"],
      "options": {
        "jestConfig": "libs/web/about/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/about/src/**/*.ts",
          "libs/web/about/src/**/*.html"
        ]
      }
    },
    "lint-html": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "hint src/**/*.html"
        ],
        "cwd": "libs/web/about",
        "parallel": false
      }
    }
  },
  "tags": []
}
